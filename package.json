{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lighthouse": "npm run lighthouse:home && npm run lighthouse:about && npm run lighthouse:contact && npm run lighthouse:projectx && npm run lighthouse:reactPlayground && npm run lighthouse:learn",
    "lighthouse:prod": "npm run lighthouse:home:prod && npm run lighthouse:about:prod && npm run lighthouse:contact:prod && npm run lighthouse:projectx:prod && npm run lighthouse:reactPlayground:prod && npm run lighthouse:learn:prod",
    "git:pre-push": "npm run build && npm run lint && npm run format && npm run lighthouse",
    "git:pre-commit": "npm run build && npm run lint && npm run format",
    "dev": "next dev",
    "prod": "next build && next start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lighthouse:home": "lighthouse http://localhost:3000/ --quiet --output html --output-path analytics/dev/homepage.html",
    "lighthouse:home:prod": "lighthouse https://web-client-ai-res.vercel.app/ --quiet --output html --output-path analytics/prod/homepage.html",
    "lighthouse:about": "lighthouse http://localhost:3000/about --quiet --output html --output-path analytics/dev/about.html",
    "lighthouse:about:prod": "lighthouse https://web-client-ai-res.vercel.app/about --quiet --output html --output-path analytics/prod/about.html",
    "lighthouse:contact": "lighthouse http://localhost:3000/support --quiet --output html --output-path analytics/dev/contact.html",
    "lighthouse:contact:prod": "lighthouse https://web-client-ai-res.vercel.app/support --quiet --output html --output-path analytics/prod/contact.html",
    "lighthouse:projectx": "lighthouse http://localhost:3000/projectx --quiet --output html --output-path analytics/dev/projects.html",
    "lighthouse:projectx:prod": "lighthouse https://web-client-ai-res.vercel.app/projectx --quiet --output html --output-path analytics/prod/projects.html",
    "lighthouse:reactPlayground": "lighthouse http://localhost:3000/react-playground --quiet --output html --output-path analytics/dev/react-playground.html",
    "lighthouse:reactPlayground:prod": "lighthouse https://web-client-ai-res.vercel.app/react-playground --quiet --output html --output-path analytics/prod/react-playground.html",
    "lighthouse:learn": "lighthouse http://localhost:3000/learn --quiet --output html --output-path analytics/dev/learn.html",
    "lighthouse:learn:prod": "lighthouse https://web-client-ai-res.vercel.app/learn --quiet --output html --output-path analytics/learn.html"
  },
